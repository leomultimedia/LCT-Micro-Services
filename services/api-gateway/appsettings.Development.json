{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Ocelot": "Debug"
    }
  },
  "Ocelot": {
    "GlobalConfiguration": {
      "BaseUrl": "http://localhost:5001"
    }
  },
  "AzureAdB2C": {
    "Instance": "https://dev-tenant.b2clogin.com",
