{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Ocelot": "Information"
    }
  },
  "Ocelot": {
    "GlobalConfiguration": {
      "BaseUrl": "https://staging-api.ecommerce.com"
    }
  },
  "AzureAdB2C": {
    "Instance": "https://staging-tenant.b2clogin.com",
    "ClientId": "staging-client-id",
    "Domain": "staging-tenant.onmicrosoft.com"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=staging-sql.database.windows.net;Database=ECommerce_Staging;User Id=staging-user;Password=staging-password;"
  },
  "ServiceBus": {
    "ConnectionString": "Endpoint=sb://staging-namespace.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=staging-key",
    "QueueName": "notifications-staging"
  },
  "Storage": {
    "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=stagingstorage;AccountKey=staging-key;EndpointSuffix=core.windows.net",
    "ContainerName": "products-staging"
  }
} 