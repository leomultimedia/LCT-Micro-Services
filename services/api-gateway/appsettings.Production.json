{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "Ocelot": "Warning"
    }
  },
  "Ocelot": {
    "GlobalConfiguration": {
      "BaseUrl": "https://api.ecommerce.com"
    }
  },
  "AzureAdB2C": {
    "Instance": "https://prod-tenant.b2clogin.com",
    "ClientId": "prod-client-id",
    "Domain": "prod-tenant.onmicrosoft.com"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=prod-sql.database.windows.net;Database=ECommerce_Prod;User Id=prod-user;Password=prod-password;"
  },
  "ServiceBus": {
    "ConnectionString": "Endpoint=sb://prod-namespace.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=prod-key",
    "QueueName": "notifications-prod"
  },
  "Storage": {
    "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=prodstorage;AccountKey=prod-key;EndpointSuffix=core.windows.net",
    "ContainerName": "products-prod"
  }
} 